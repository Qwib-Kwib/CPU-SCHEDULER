<Window x:Class="Info_module.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Info_module"
        mc:Ignorable="d"
        Title="Info-Module" Height="450" Width="800"
        MinHeight="450" MinWidth="800" WindowState="Maximized" SizeChanged="Window_SizeChanged">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Frame x:Name="MainFrame" Content="" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="SideFrame" Content="" NavigationUIVisibility="Hidden" IsHitTestVisible="False"/>

    </Grid>
</Window>
